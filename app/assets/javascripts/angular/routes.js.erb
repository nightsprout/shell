AngularApp.config(function($routeProvider, $locationProvider) {
  $routeProvider.when('/', {
    templateUrl: '<%= asset_path("index.html") %>',
    controller: 'indexController',
    activetab: 'index'
  }).when('/champion_maker', {
    templateUrl: '<%= asset_path("champion_maker.html") %>',
    controller: 'indexController',
    activetab: 'champion_maker'
  }).when('/masteries', {
    templateUrl: '<%= asset_path("masteries.html") %>',
    controller: 'masteriesController',
    activetab: 'masteries'
  }).when('/damage', {
    templateUrl: '<%= asset_path("damage.html") %>',
    controller: 'indexController',
    activetab: 'damage'
  }).when('/about', {
    templateUrl: '<%= asset_path("about.html") %>',
    controller: 'indexController',
    activetab: 'about'
  }).
  otherwise({
    templateUrl: '<%= asset_path("index.html") %>',
    activetab: 'index'
  });
  $locationProvider.html5Mode(true);
});
































































AngularApp.config(function($routeProvider, $locationProvider) {
  $routeProvider.when('/', {
    templateUrl: '<%= asset_path("index.html") %>',
    controller: 'indexController',
    activetab: 'index'
  }).
  otherwise({
    templateUrl: '<%= asset_path("index.html") %>',
    activetab: 'index'
  });
  $locationProvider.html5Mode(true);
});
































































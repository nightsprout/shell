AngularApp.config(["$routeProvider", "$locationProvider", function($routeProvider, $locationProvider) {
  $routeProvider.when('/', {
    templateUrl: '<%= asset_path("index.html") %>',
    activetab: 'index'
  }).
  otherwise({
    templateUrl: '<%= asset_path("index.html") %>',
    activetab: 'index'
  });
  $locationProvider.html5Mode(true);
}]);































































